{
  "grammars": [
    {
      "name": "django",
      "camelcase": "Django",
      "scope": "source.django",
      "path": ".",
      "file-types": [".html", ".tpl"],
      "injection-regex": "^django$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for Django templates.",
    "authors": [
      {
        "name": "William Vandervalk"
      },
      {
        "name": "Dominique PERETTI"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-django"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false
  }
}
